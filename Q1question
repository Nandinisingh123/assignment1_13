#include<iostream>      
using namespace std;
class Rectangle
{
 private:
 int length,breadth;
 public:
 Rectangle()
 {
   length=0;
   breadth=0;
 }
 Rectangle(int a,int b)
 {
    length=a;
    breadth=b;
 }
 Rectangle(int a)
 {
    length=a;
    breadth=a
 }
 int area()
 {
    return length*breadth;
 }
};

int main()
{
  Rectangle cal1;
  Rectangle cal2(2,3);
  Rectangle cal3(5);
  cout<<cal1.area()<<endl;
  cout<<cal2.area()<<endl;
  cout<<cal3.area()<<endl;
}
